{
  "name": "danilowa/laravel-easy-cloud-storage",
  "version": "1.0.0",
  "description": "A robust and flexible Laravel package for managing files in various cloud storage providers.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "homepage": "https://github.com/DaniloWA/laravel-easy-cloud-storage",
  "keywords": [
    "DaniloWA",
    "danilo",
    "oliveira",
    "laravel-easy-cloud-storage",
    "easy-cloud-storage",
    "easy-cloud",
    "easy-storage",
    "cloud-storage",
    "laravel",
    "api",
    "storage",
    "file-storage",
    "file",
    "image-storage"
  ],
  "authors": [
    {
      "name": "Danilo Oliveira",
      "email": "daniloworkdev@gmail.com",
      "homepage": "http://www.daniloo.dev",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1|^8.2",
    "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Danilowa\\LaravelEasyCloudStorage\\": "src/",
      "Danilowa\\LaravelEasyCloudStorage\\Config\\": "src/Config/",
      "Danilowa\\LaravelEasyCloudStorage\\Contracts\\": "src/Contracts/",
      "Danilowa\\LaravelEasyCloudStorage\\Exceptions\\": "src/Exceptions/",
      "Danilowa\\LaravelEasyCloudStorage\\Facades\\": "src/Facades/",
      "Danilowa\\LaravelEasyCloudStorage\\Providers\\": "src/Providers/",
      "Danilowa\\LaravelEasyCloudStorage\\Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Danilowa\\LaravelEasyCloudStorage\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Danilowa\\LaravelEasyCloudStorage\\Providers\\EasyCloudStorageServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "dist"
  }
}
